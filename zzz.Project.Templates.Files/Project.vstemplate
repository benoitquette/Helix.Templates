    </CustomParameters>
    <ProjectCollection>
      <SolutionFolder Name="$safeprojectname$">
        <ProjectTemplateLink ProjectName="Sitecore.$layer$.$modulename$" CopyParameters="true">code\CodeProject.vstemplate</ProjectTemplateLink>
      </SolutionFolder>
      <SolutionFolder Name="serialization">
        <SolutionFolder Name="$layer$.$safeprojectname$.Content">
          
        </SolutionFolder>
      </SolutionFolder>
    </ProjectCollection>
  </TemplateContent>
  <WizardExtension>
    <Assembly>Sitecore.Helix.Templates, Version=1.0.0.0, Culture=neutral, PublicKeyToken=8896e856cf3fdc7d</Assembly>
    <FullClassName>Sitecore.Helix.Templates.Wizard</FullClassName>
  </WizardExtension>
</VSTemplate>