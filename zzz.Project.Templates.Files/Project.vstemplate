  <TemplateData>
    <Name>Sitecore Project Module</Name>
    <Description>Sitecore Foundation Module</Description>
    <Icon>window.png</Icon>
    <ProjectType>CSharp</ProjectType>
    <RequiredFrameworkVersion>2.0</RequiredFrameworkVersion>
    <SortOrder>1000</SortOrder>
    <TemplateID>fb5061d3-7ef1-429e-af26-9015dee8726a</TemplateID>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Module</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Hidden>false</Hidden>
  </TemplateData>
  <TemplateContent>
    <CustomParameters>
      <CustomParameter Name="$layer$" Value="Project"/>
      <CustomParameter Name="$prefixedlayer$" Value="zzz.Project"/>
