  </TemplateContent>
  <WizardExtension>
    <Assembly>Helix.Templates, Version=1.0.0.0, Culture=neutral, PublicKeyToken=8896e856cf3fdc7d</Assembly>
    <FullClassName>Helix.Templates.Wizard</FullClassName>
